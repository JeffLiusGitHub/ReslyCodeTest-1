{
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"requireConfigFile": false,
		"func-style": ["error", "expression"],
		"ecmaFeatures": {
			"jsx": true
		},
		"env": {
			"es6": true,
			"browser": true
		},
		"globals": {
			"__DEV_MODEL__": true,
			"__DEV__": true,
			"__QA__": true,
			"__YZ__": true,
			"__DEMO__": true,
			"__PROD__": true
		}
	}
}
